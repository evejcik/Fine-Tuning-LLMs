    "def sample_data(df, num_samples_per_class):\n",
    "    sampled_df = df.groupby(\"labels\", group_keys=False).apply(lambda x: x.sample(num_samples_per_class, replace=True))\n",
    "    return sampled_df.reset_index(drop=True)"
   ]
